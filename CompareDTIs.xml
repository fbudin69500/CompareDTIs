<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Filtering</category>
  <title>ValueAtPos</title>
  <description>
   This module prints the value at a voxel position of the image.
  </description>
  <version>1</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor>Francois Budin</contributor>
<acknowledgements>
</acknowledgements>
  <parameters>
    <label>Input/Output</label>
    <description>Input/output parameters</description>
    <image>
      <name>inputVolume1</name>
      <label>Input Volume 1</label>
      <channel>input</channel>
      <index>0</index>
      <description>Reference Input Image</description>
    </image>
    <image>
      <name>inputVolume2</name>
      <label>Input Volume 2</label>
      <channel>input</channel>
      <index>0</index>
      <description>Input Image compared</description>
    </image>
    <image>
      <name>mask</name>
      <label>Mask</label>
      <description>Mask Image</description>
      <flag>m</flag>
      <longflag>mask</longflag>
    </image>
    <image>
      <name>outputVolume</name>
      <label>Output Volume</label>
      <description>Output Image</description>
      <flag>o</flag>
      <longflag>output</longflag>
    </image>
