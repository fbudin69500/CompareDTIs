      <default>div</default>
      <element>div</element>
      <element>sub</element>
      <element>subref</element>
      <element>subcomp</element>
    </string-enumeration>
    <string-enumeration>
      <name>ev</name>
      <label>eigen-vector</label>
      <description>eigen-vector</description>
      <longflag>ev</longflag>
      <default>1</default>
      <element>1</element>
      <element>2</element>
      <element>3</element>
    </string-enumeration>
  </parameters>
